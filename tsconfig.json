{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "lib": [
      "ES5",
      "DOM"
    ],
    "allowJs": true,
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist",
    "rootDir": "./src",
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "rootDirs": [
      "src"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src",
    "src/utils.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
